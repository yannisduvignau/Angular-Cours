{
  "name": "projet-bachelor",
  "version": "0.0.0",
  "scripts": {
    "serve:ng": "ng serve --port 54675",
    "start": "concurrently \"npm run serve:ng\" \"npm run serve:json\"",
    "build": "ng build",
    "watch": "ng build --watch --configuration development",
    "test": "ng test",
    "serve:ssr:projet_Bachelor": "node dist/projet-bachelor/server/server.mjs",
